G:=PermutationGroup<4|1,(1,2)(3,4),(1,3)(2,4),(1,4)(2,3)>;
r:= RootOfUnity(4);
w:= RootOfUnity(8);
X:={1,2,3,4};
b:=func<x|(-1+(-1)^(x))*x div 2>;
f:=func<x|r^(b(x))>;
h:=func<a,x|r^(b(x^G.a))>;
for a in X do
sum:=0;
for x in X do
	sum+:=h(a,x)*(f(x))^(-1);
end for;
sum;
end for;
CT := CharacterTable(G);
chi := CT[2];
CT;
Y:=GSet(G);
Y;
IsRegular(G,Y);
IsTransitive(G,Y);
